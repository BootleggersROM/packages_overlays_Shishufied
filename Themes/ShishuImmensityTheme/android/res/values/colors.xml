<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <!-- Light values -->
    <color name="background_material_light">#DDF9F9F9</color>
    <color name="background_floating_material_light">@*android:color/background_material_light</color>
    <color name="primary_material_light">#4fF9F9F9</color>
    <!-- Workaround for light themed settings background -->
    <color name="primary_material_settings_light">#4fF9F9F9</color>
    <color name="primary_dark_material_light">@*android:color/transparent</color>
    <color name="primary_dark_material_settings_light">@*android:color/transparent</color>
    <!-- The background color of a notification card. -->
    <color name="notification_material_background_color">@*android:color/background_material_light</color>
    <!-- Notification default icon color -->
    <color name="notification_default_color_light">@*android:color/accent_device_default_light</color>
    <!-- Nuke those dividers -->
    <color name="list_divider_color_light">#00000000</color>
    <color name="list_divider_opacity_device_default_light">#00000000</color>

    <!-- Dark values -->
    <color name="background_material_dark">#CD070707</color>
    <color name="background_floating_material_dark">@*android:color/background_material_dark</color>
    <color name="primary_material_dark">#4f070707</color>
    <color name="primary_material_settings">@*android:color/background_material_dark</color>
    <color name="primary_dark_material_dark">@*android:color/transparent</color>
    <color name="primary_dark_material_settings">@*android:color/transparent</color>
    <!-- Notification default icon color -->
    <color name="notification_default_color_dark">@*android:color/background_material_dark</color>
    <!-- Nuke those dividers -->
    <color name="list_divider_color_dark">#00000000</color>
    <color name="list_divider_opacity_device_default_dark">#00000000</color>
</resources>
